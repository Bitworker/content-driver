- data_blocks.each do |block|
  - if block[1][:p_tags].present?
    .block{ :data => { :url => url } }
      - block[1][:headlines][:h1_tags].each do |h1|
        %h1= h1
      - block[1][:headlines][:h2_tags].each do |h2|
        %h2= h2
      - block[1][:headlines][:h3_tags].each do |h3|
        %h3= h3
      - block[1][:p_tags].each do |p|
        %p= p
      - block[1][:a_tags].each do |a|
        %a{ :href => a[0][:href] }= a[0][:text]
      - block[1][:img_tags].each do |img|
        %img{ :src => img }
